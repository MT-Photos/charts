            "type": "list",
            "items": [
              {
                "variable": "extraAppVolume",
                "label": "目录映射",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "mountPath",
                      "label": "Pod内的路径",
                      "description": "在pod内挂载的路径",
                      "schema": {
                        "type": "path",
                        "required": true
                      }
                    },
                    {
                      "variable": "hostPath",
                      "label": "宿主机路径",
                      "description": "存储着已有照片的目录",
