            "type": "dict",
            "attrs": [
              {
                "variable": "config",
                "label": "/config 数据目录",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "datasetName",
                      "label": "Configuration Volume Dataset Name",
                      "schema": {
                        "type": "string",
                        "hidden": true,
                        "$ref": [
                          "normalize/ixVolume"
                        ],
                        "show_if": [
                          [
                            "hostPathEnabled",
                            "=",
                            false
                          ]
                        ],
                        "default": "ix-mtphotos_config",
                        "editable": false
                      }
                    },
                    {
                      "variable": "mountPath",
                      "label": "Pod内的路径",
                      "schema": {
                        "type": "path",
                        "hidden": false,
                        "editable": false,
                        "default": "/config"
                      }
                    },
                    {
                      "variable": "hostPathEnabled",
                      "label": "指定目录映射位置",
                      "description": "关闭时系统会为其自动创建数据集",
                      "schema": {
                        "type": "boolean",
                        "default": true,
                        "show_subquestions_if": true,
                        "subquestions": [
                          {
                            "variable": "hostPath",
                            "label": "存储MT Photos数据的目录",
                            "schema": {
                              "type": "hostpath",
                              "required": true
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              },
