    "schema": {
      "groups": [
        {
          "name": "Networking",
          "description": "Networking Configuration for MT Photos"
        },
        {
          "name": "Storage",
          "description": "容器内部存储的生存周期和容器一样，容器删除时，容器内的存储也会删除，任何保存于容器内部存储的文件都会随容器删除而丢失。\n\n因此创建容器时需要使用数据卷（Volume）或者目录映射 来存储文件；"
        },
        {
          "name": "Configuration",
          "description": "MT Photos application configuration"
        },

        {
          "name": "Advanced DNS Settings",
          "description": "Configure DNS settings"
        },
        {
          "name": "Resource Limits",
          "description": "Set CPU/memory limits for Kubernetes Pod"
        }
      ],
      "portals": {
        "web_portal": {
          "protocols": [
            "http"
          ],
          "host": [
            "$node_ip"
          ],
          "ports": [
            "$variable-web_port"
          ],
          "path": "/"
        }
      },
      "questions": [
        {
          "variable": "web_port",
          "label": "用于访问MT Photos服务的端口",
          "group": "Networking",
          "schema": {
            "type": "int",
            "default": 28063,
            "min": 9000,
            "max": 65535,
            "required": true
          }
        },
        {
          "variable": "targetPort",
          "label": "Pod内的端口",
          "group": "Networking2",
          "schema": {
            "type": "int",
            "default": 8063,
            "min": 80,
            "max": 65535,
            "required": true
          }
        },
        {
          "variable": "ownerUID",
          "label": "ownerUID - 目录所属用户UID",
          "group": "Configuration",
          "description": "",
          "schema": {
            "type": "int",
            "default": 1000,
            "min": 1,
            "max": 65535
          }
        },
        {
          "variable": "ownerGID",
          "label": "ownerGID - 目录所属用户GID",
          "group": "Configuration",
          "description": "",
          "schema": {
            "type": "int",
            "default": 1000,
            "min": 1,
            "max": 65535
          }
        },
        {
          "variable": "TZ",
          "label": "时区",
          "group": "Configuration",
          "description": "",
          "schema": {
            "type": "string",
            "default": "Asia/Shanghai",
            "$ref": [
              "definitions/timezone"
            ]
          }
        },
        {
          "variable": "environmentVariables",
          "label": "MT Photos 环境变量",
          "group": "Configuration",
          "schema": {
            "type": "list",
            "default": [],
            "items": [
              {
                "variable": "environmentVariable",
                "label": "Environment Variable",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "name",
                      "label": "Name",
                      "schema": {
                        "type": "string"
                      }
                    },
                    {
                      "variable": "value",
                      "label": "Value",
                      "schema": {
                        "type": "string"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "appVolumeMounts",
          "label": "MT Photos 目录映射",
          "group": "Storage",
          "schema": {
            "type": "dict",
            "attrs": [
              {
                "variable": "config",
                "label": "MT Photos数据存储目录",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "datasetName",
                      "label": "config Storage Volume Dataset Name",
                      "schema": {
                        "type": "string",
                        "hidden": true,
                        "$ref": [
                          "normalize/ixVolume"
                        ],
                        "show_if": [
                          [
                            "hostPathEnabled",
                            "=",
                            false
                          ]
                        ],
                        "default": "ix-mtphotos_config",
                        "editable": false
                      }
                    },
                    {
                      "variable": "mountPath",
                      "label": "Pod内的路径",
                      "description": "",
                      "schema": {
                        "type": "path",
                        "hidden": true,
                        "editable": true,
                        "default": "/config"
                      }
                    },
                    {
                      "variable": "hostPathEnabled",
                      "label": "指定目录映射位置 /config",
                      "schema": {
                        "type": "boolean",
                        "default": true,
                        "show_subquestions_if": true,
                        "subquestions": [
                          {
                            "variable": "hostPath",
                            "label": "存储MT Photos数据的目录",
                            "schema": {
                              "type": "hostpath",
                              "required": true
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              },
              {
                "variable": "upload",
                "label": "MT Photos照片备份目录",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "datasetName",
                      "label": "upload Storage Volume Dataset Name",
                      "schema": {
                        "type": "string",
                        "hidden": true,
                        "$ref": [
                          "normalize/ixVolume"
                        ],
                        "show_if": [
                          [
                            "hostPathEnabled",
                            "=",
                            false
                          ]
                        ],
                        "default": "ix-mtphotos_data",
                        "editable": false
                      }
                    },
                    {
                      "variable": "mountPath",
                      "label": "Pod内的路径",
                      "description": "",
                      "schema": {
                        "type": "path",
                        "hidden": true,
                        "editable": true,
                        "default": "/upload"
                      }
                    },
                    {
                      "variable": "hostPathEnabled",
                      "label": "指定目录映射位置",
                      "schema": {
                        "type": "boolean",
                        "default": true,
                        "show_subquestions_if": true,
                        "subquestions": [
                          {
                            "variable": "hostPath",
                            "label": "存放App备份的照片目录 /upload",
                            "schema": {
                              "type": "hostpath",
                              "required": true
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "extraAppVolumeMounts",
          "label": "添加其他目录映射",
          "group": "Storage",
          "schema": {
            "type": "list",
            "items": [
              {
                "variable": "extraAppVolume",
                "label": "Host Path Volume",
                "description": "Add an extra host path volume for MT Photos application",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "mountPath",
                      "label": "Pod内的路径",
                      "description": "在pod内挂载的路径",
                      "schema": {
                        "type": "path",
                        "required": true
                      }
                    },
                    {
                      "variable": "readOnly",
                      "label": "Read Only",
                      "description": "Mount volume as read only",
                      "schema": {
                        "type": "boolean",
                        "default": false
                      }
                    },
                    {
                      "variable": "hostPath",
                      "label": "宿主机路径",
                      "description": "存储着已有照片的目录",
                      "schema": {
                        "type": "hostpath",
                        "required": true
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "dnsConfig",
          "label": "DNS Configuration",
          "group": "Advanced DNS Settings",
          "schema": {
            "type": "dict",
            "attrs": [
              {
                "variable": "options",
                "label": "DNS Options",
                "schema": {
                  "type": "list",
                  "items": [
                    {
                      "variable": "optionsEntry",
                      "label": "Option Entry Configuration",
                      "schema": {
                        "type": "dict",
                        "attrs": [
                          {
                            "variable": "name",
                            "label": "Option Name",
                            "schema": {
                              "type": "string",
                              "required": true
                            }
                          },
                          {
                            "variable": "value",
                            "label": "Option Value",
                            "schema": {
                              "type": "string",
                              "required": true
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "enableResourceLimits",
          "label": "Enable Pod resource limits",
          "group": "Resource Limits",
          "schema": {
            "type": "boolean",
            "default": false
          }
        },
        {
          "variable": "cpuLimit",
          "label": "CPU Limit",
          "description": "CPU resource limit allow  plain integer values with suffix m(milli) e.g 1000m, 100.",
          "group": "Resource Limits",
          "schema": {
            "type": "string",
            "show_if": [
              [
                "enableResourceLimits",
                "=",
                true
              ]
            ],
            "valid_chars": "^\\d+(?:\\.\\d+(?!.*m$)|m?$)",
            "default": "4000m"
          }
        },
        {
          "variable": "memLimit",
          "label": "Memory Limit",
          "group": "Resource Limits",
          "description": "Memory limits is specified by number of bytes. Followed by quantity suffix like E,P,T,G,M,k and Ei,Pi,Ti,Mi,Gi,Ki can also be used. e.g 129e6, 129M,  128974848000m, 123Mi",
          "schema": {
            "type": "string",
            "show_if": [
              [
                "enableResourceLimits",
                "=",
                true
              ]
            ],
            "valid_chars": "^([+-]?[0-9.]+)([eEinumkKMGTP]*[-+]?[0-9]*)$",
            "default": "8Gi"
          }
        }
      ]
    },
    "app_readme": "<h1>MT Photos</h1>\n<p><a href=\"https://mtmt.tech\">MT Photos照片管理系统</a></p>",
